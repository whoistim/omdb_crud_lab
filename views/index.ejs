<!doctype html>
<html>
  <head>
    <title>OMDB API Example</title>
  </head>
  <body>
  <h1>Find Movies!</h1>
    <!-- action is the URL we send our data to -->
    <!-- method is the HTTP verb (e.g., GET, POST) -->
    <form action="/search" method="GET">
      <input type="text" name="movieTitle" placeholder="enter a movie name">
      <input type="submit">
    </form>
  </body>
</html>
